{
  "version": 2,
  "builds": [
    {
      "src": "frontend/angular.json",
      "use": "@vercel/angular"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/(.*)", "dest": "/frontend/dist/frontend/$1" }
  ]
}
